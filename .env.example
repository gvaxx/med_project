LLM_SERVICE_PORT=8003
AUDIO_SERVICE_PORT=8004
STREAMLIT_PORT=8501
INTEGRATED_STREAMLIT_PORT=8502

# Healthcheck Configuration
HEALTHCHECK_INTERVAL=30s
HEALTHCHECK_TIMEOUT=10s
